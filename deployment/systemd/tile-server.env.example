# Copy to: /etc/twf-v3/tile-server.env
TWF_V3_DATA_ROOT=/opt/twf_v3/data/v3

# Canonical boundaries/hydro vector tileset for /tiles/v3/boundaries/v1/*
TWF_V3_BOUNDARIES_MBTILES=/opt/twf_v3/data/v3/boundaries/v1/twf_boundaries.mbtiles
TWF_V3_BOUNDARIES_TILESET_ID=twf-boundaries-v1
TWF_V3_BOUNDARIES_TILESET_NAME=TWF Boundaries v1
TWF_V3_TILES_PUBLIC_BASE_URL=https://api.theweathermodels.com

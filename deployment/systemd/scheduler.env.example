# Copy to: /etc/twf-v3/scheduler.env
# Required runtime paths
TWF_V3_DATA_ROOT=/opt/twf_v3/data/v3
TWF_V3_WORKERS=4

# Optional scheduler defaults (used when not passed on ExecStart)
TWF_V3_SCHEDULER_VARS=tmp2m,wspd10m,refc,radar_ptype
TWF_V3_SCHEDULER_PRIMARY_VARS=tmp2m
TWF_V3_SCHEDULER_POLL_SECONDS=300
TWF_V3_SCHEDULER_KEEP_RUNS=2
